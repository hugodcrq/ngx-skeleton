{
  "name": "@hugodcrq/ngx-skeleton",
  "version": "0.0.0-development",
  "description": "âŒ›Angular library to easily create skeleton loadings without SVG.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "skeleton",
    "ngx-skeleton"
  ],
  "author": {
    "name": "Hugo Decroocq"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hugodcrq/ngx-skeleton.git"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "prettier:staged": "npx pretty-quick --staged",
    "cz:noverify": "cz --no-verify",
