    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "husky": "8.0.1",
    "ng-packagr": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "3.1.3",
    "semantic-release": "^20.0.2",
    "typescript": "~4.9.4"
